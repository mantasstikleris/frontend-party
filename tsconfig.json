{
  "compilerOptions": {
    "outDir": "dist",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es5", "es6", "dom"],
    "sourceMap": true,
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strictNullChecks": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.jpg"],
  "exclude": ["node_modules", "dist"],
  "compileOnSave": true
}
